{
  "name": "plate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development npm-run-all --parallel dev-server serve",
    "dev-server": "webpack-dev-server --config config/webpack.dev.js",
    "serve": "node server/index.js",
    "clean": "rimraf ./public/build",
    "prebuild": "npm run clean",
    "build": "NODE_ENV=production npm-run-all build-client build-server",
    "build-client": "webpack --config config/webpack.prod.js --progress",
    "build-server": "webpack --config config/webpack.server.js --progress",
    "serve-prod-csr": "NODE_ENV=production SSR=0 npm run serve",
    "serve-prod-ssr": "NODE_ENV=production SSR=1 npm run serve"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.5",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "compression": "^1.6.2",
    "express": "^4.15.2",
    "isomorphic-fetch": "^2.2.1",
    "object-assign": "^4.1.1",
    "promise": "^7.1.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-promise-middleware": "^4.3.0",
    "serialize-javascript": "^1.3.0"
  }
}
